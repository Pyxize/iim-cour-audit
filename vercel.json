{
  "routes": [
    {
      "src": "index.html",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; img-src 'self' https://apis.google.com 'self' 'unsafe-eval'; style-src 'self' 'unsafe-inline'",
        "X-Frame-Options": "deny",
        "X-XSS-Protection": "1"
      }
    }
  ]
}
